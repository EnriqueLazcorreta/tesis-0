	Note = {\leePDF{bib/citas/Etzioni-TheWWWQuagmireOrGoldMine-1996.pdf} \descarga{ftp://ftp.cs.washington.edu/homes/etzioni/softbots/cacm96.ps}},
	Number = {11},
	Owner = {Enrique},
	Pages = {65--68},
	Review = {http://citeseer.ist.psu.edu/viewdoc/summary?doi=10.1.1.37.8942\&rank=1},
	Timestamp = {2014.11.18},
	Title = {{The World-Wide Web: Quagmire or Gold Mine?}},
	Url2 = {http://citeseer.ist.psu.edu/58014.html},
	Volume = {39},
	Year = {1996}}

