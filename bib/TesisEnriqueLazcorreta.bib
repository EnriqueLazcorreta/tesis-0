	Note = {\leePDF{bib/citas/LiuHsu_PostAnalysisOfLearnedRules_1996.pdf} \descarga{http://citeseer.ist.psu.edu/viewdoc/download?doi=10.1.1.42.7715&rep=rep1&type=pdf}},
	Pages = {828-834},
	Title = {Post-Analysis of Learned Rules},
