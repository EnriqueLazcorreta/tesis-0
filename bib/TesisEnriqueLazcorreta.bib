@article{CooleyMobasherSrivastava_DataPreparationForMiningWWWBrowsingPatterns_1999,
	Abstract = {The World Wide Web (WWW) continues to grow at an astounding rate in both the sheer volume of traffic and the size and complexity of Web sites. The complexity of tasks such as Web site design, Web server design, and of simply navigating through a Web site have increased along with this growth. An important input to these design tasks is the analysis of how a Web site is being used. Usage analysis includes straightforward statistics, such as page access frequency, as well as more sophisticated forms of analysis, such as finding the common traversal paths through a Web site. Web Usage Mining is the application of data mining techniques to usage logs of large Web data repositorioes in order to produce results that can be used in the design tasks mentioned above. However, there are several preprocessing tasks that must be performed prior to applying data mining algorithms to the data collected from server logs. This paper presents several data preparation techniques in order to identify unique users and user sessions. Also, a method to divide user sessions into semantically meaningful transactions is defined and successfully tested against two other methods. Transactions identified by the proposed methods are used to discover association rules from real world data using the WEBMINER system [15].},
	Author = {Robert Cooley and Bamshad Mobasher and Jaideep Srivastava},
	Citeseer = {http://citeseer.ist.psu.edu/cooley99data.html},
	File = {CooleyMobasherSrivastava_DataPreparationForMiningWWWBrowsingPatterns_1999.pdf:citas\\CooleyMobasherSrivastava_DataPreparationForMiningWWWBrowsingPatterns_1999.pdf:PDF},
	Iddea = {017},
	Journal = {Knowledge and Information Systems},
	Note = {\leePDF{bib/citas/CooleyMobasherSrivastava_DataPreparationForMiningWWWBrowsingPatterns_1999.pdf} \descarga{http://facweb.cs.depaul.edu/mobasher/classes/ect584/papers/cms-kais.pdf}},
	Num = {1},
	Owner = {Enrique},
	Pages = {5--32},
	Review = {Pre-proceso de los ficheros de log, identificación de usuarios, creación de sesiones, conversión en transacciones y descubrimiento de reglas de asociación. Indica que \cite{Chen:Park:Yu:DataMiningForPathTraversalPatterns:96} propone el método Maximal Forward Reference y ellos lo mejoran proponiendo Reference Lenght, ambos mejores que Time Windows (el que usamos nosotros) en todos los aspectos. Destaca la importancia del pre-proceso, sobre todo la reconstrucción de las transacciones con que se trabajará, eliminando del estudio las páginas auxiliares visitadas y obteniendo menos reglas y de mayor calidad. Los métodos estudiados determinan por sí mismos qué páginas son auxiliares y cuáles de contenido, en base al conocimiento de la topología del sitio web estudiado.},
	Timestamp = {2014.11.28},
	Title = {{Data Preparation for Mining World Wide Web Browsing Patterns}},
	Vol = {1},
	Year = {1999}}

@inproceedings{CooleyMobasherSrivastava_WebMining_1997,
	Abstract = {Application of data mining techniques to the World Wide Web, referred to as Web mining, has been he focus of several recent research projects and papers. However, there is no stablished vocabulary, lading to confusion when comparing research efforts. The term Web mining has been used in two distintict ways. The first, called Web content mining in this paper, is the process of information discovery from sources across the Worl Wide Web. The second, called Web Usage Mining, is the process of mining for user browsing and access patterns. In this paper we define Web mining and presenta an overview of the various research issues, techniques, and development efforts. We briefly describe WEBMINER, a system for Web usage mining, and conclude this paper by listing research issues.},
