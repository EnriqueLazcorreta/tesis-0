@inproceedings{PerkowitzEtzioni_AWSConceptualClusterMining_1999,
	Abstract = {The creation of a complex web site is a thorny problem in user interface design. In IJCAI'97, we challenged the AI community to address this problem by creating adaptive web sites. In response, we investigate the problem of index page synthesis --- the automatic creation of pages that facilitate a visitor's navigation of a Web site. Previous work has employed statistical methods to generate candidate index pages that are of limited value because they do not correspond to concepts or topics that are intuitive to people. In this paper we formalize index page synthesis as a conceptual clustering problem and introduce a novel approach which we call {\em conceptual cluster mining}: we search for a small number of cohesive clusters that correspond to concepts in a given concept description language $L$.\\ Next, we present SCML, an algorithm schema that combines a statistical clustering algorithm with a concept learning algorithm. The clustering algorithm is used to generate seed clusters, and the concept learning algorithm to describe these seed clusters using expressions in $L$. Finally, we offer preliminary experimental form existing algorithms (e.g., COBWEB) in this domain.},
	Author = {Mike Perkowitz and Oren Etzioni},
	Booktitle = {{IJCAI}},
	Citeseer = {http://citeseer.ist.psu.edu/196899.html},
	Date-Added = {2014-12-20 18:49:38 +0000},
	Date-Modified = {2014-12-20 19:06:04 +0000},
	File = {PerkowitzEtzioni_AWSConceptualClusterMining_99:PerkowitzEtzioni_AWSConceptualClusterMining_99:PDF},
	Keywords = {web log, web usage mining, adaptive web site, clustering},
	Pages = {264--269},
	Title = {{Adaptive Web Sites: Conceptual Cluster Mining}},
	Year = {1999}}

@inproceedings{PerkowitzEtzioni_AWSAutomaticallySynthesizingWebPages_1998,
	Abstract = {The creation of a complex web site is a thorny problem in user interface design. In IJCAI'97, we challenged the AI community to address this problem by creating adaptive web sites: sites that automatically improve their organization and presentation by mining visitor access data collected in Web server logs. In this paper we introduce our own approach to this broad challenge. Specifically, we investigate the problem of index page synthesis --- the automatic creation of pages that facilitate a visitor's navigation of a Web site. First, we formalize this problem as a clustering problem and introduce a novel approach to clustering, which we call cluster mining: Instead of attempting to partition the entire data space into disjoint clusters, we search for a small number of cohesive (and possibly overlapping) clusters. Next, we present PageGather, a cluster mining algorithm that takes Web server logs as input and outputs the contents of candidate index pages. Finally, we show experimentally that PageGather is both faster (by a factor of three) and more effective than traditional clustering algorithms on this task. Our experiment relies on access log collected over a month from an actual web site.},
	Annote = {ISBN 0-262-51098-7
URL http://citeseer.ist.psu.edu/viewdoc/summary?doi=10.1.1.27.2249&rank=3},
	Author = {Mike Perkowitz and Oren Etzioni},
